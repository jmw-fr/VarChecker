{
  "title": "VarChecker",
  "authors": [ "Jean-Marc Weeger" ],
  "description": "Fluent assertion library for variables",
  "version": "0.2.1-*",

  "dependencies": {
    "NETStandard.Library": "1.6.0",
    "StyleCop.Analyzers": "1.0.0"
  },

  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "dotnet5.6",
        "portable-net45+win8"
      ]
    },
    "netstandard1.6": {
      "imports": "dnxcore50"
    },
    "net40": {},
    "net45": {},
    "net451": {},
    "net461": {}
  },
  "buildOptions": {
    "xmlDoc": true,
    "additionalArguments": [
      "/ruleset:../../stylecop.ruleset",
      "/additionalfile:../../stylecop.json"
    ]
  },
  "packOptions": {
    "summary": "This library simplifies the check of input variables in functions by allowing fluent assertions.",
    "owners": [ "Jean-Marc Weeger" ],
    "projectUrl": "https://github.com/jmw-fr/VarChecker",
    "licenseUrl": "https://opensource.org/licenses/mit-license.php",
    "releaseNotes": "",
    "requireLicenseAcceptance": false,
    "repository": {
      "url": "https://github.com/jmw-fr/VarChecker",
      "type": "git"
    },
    "iconUrl": "https://jmw-fr.github.io/VarChecker/images/varchecker.png"
  }
}
